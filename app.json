{
  "expo": {
    "name": "cleancard",
    "slug": "cleancard",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": ["cleancard"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Shuffle to access your camera roll.",
          "cameraPermission": "Allow Shuffle to access your camera"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
